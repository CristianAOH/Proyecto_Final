<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contenido_notas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/linea4"
    tools:context="itchihuahua.mitec.com.mitec.Notas">


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabtn_agregar"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="36dp"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        app:backgroundTint="#FFAB00"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/masnota" />

    <ListView
        android:id="@+id/lstV_notas"
        android:layout_width="317dp"
        android:layout_height="554dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>
